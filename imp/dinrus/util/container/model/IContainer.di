module util.container.model.IContainer;

/*******************************************************************************

        Generic container
        
*******************************************************************************/

interface ИКонтейнер (V)
{
        т_мера размер ();

        бул пуст_ли ();

        ИКонтейнер dup ();

        ИКонтейнер очисть ();                          

        ИКонтейнер сбрось ();                          

        ИКонтейнер проверь ();

        бул содержит (V значение);

        бул возьми (ref V элемент);

        V[] вМассив (V[] приёмн = пусто);

        т_мера удали (V элемент, бул все);

        цел opApply (цел delegate(ref V значение) дг);

        т_мера замени (V старЭлемент, V новЭлемент, бул все);
}


/*******************************************************************************

        Сравнитель function
        
*******************************************************************************/

template Сравни (V)
{
        alias цел function (ref V a, ref V b) Сравни;
}

