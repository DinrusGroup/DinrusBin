module col.model.Multi;
public import col.model.Collection,
       col.model.Addable;

/**
 * Интерфейс Мульти задаёт функции для объектов, которые одновременно
 * могут находиться в нескольких экземплярах.
 */
interface Мульти(V)
{
    /**
     * посчитать число элементов, соответствующих данному значению
     */
    бцел счёт(V v);

    /**
     * удалить все экземпляры данного элемента.
     */
    Мульти!(V) удалиВсе(V v);

    /**
     * удалить все экземпляры данного элемента.
     *
     * чло_Удалённых устанавливается в число удалённых элементов.
     */
    Мульти!(V) удалиВсе(V v, ref бцел чло_Удалённых);
}
